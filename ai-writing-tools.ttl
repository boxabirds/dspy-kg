@prefix ex: <http://example.org/> .
@prefix jasper: <https://jasper.ai#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix schema: <http://schema.org/> .
@prefix dct: <http://purl.org/dc/terms/> .

ex:Grammarly a ex:AIWritingTool ;
    rdfs:label "Grammarly" ;
    ex:hasFeature ex:TextImprovement ;
    ex:hasFeature ex:PersonalizedSuggestions ;
    ex:hasFeature ex:DataSecurity ;
    ex:trustedBy ex:Organization1, ex:Organization2, ex:Organization3 ;
    ex:website <https://grammarly.com> ;
    ex:description "Grammarly makes AI writing convenient. Work smarter with personalized AI guidance and text generation on any app or website." ;
    prov:wasDerivedFrom <https://grammarly.com> .

jasper:Jasper a ex:AIWritingTool ;
    rdfs:label "Jasper" ;
    schema:description "A purpose-built AI platform for marketing that helps enterprise marketers build AI-powered apps and workflows tied to real business outcomes." ;
    schema:applicationCategory "Marketing" ;
    schema:operatingSystem "Web" ;
    schema:offers jasper:FreeTrial ;
    prov:wasDerivedFrom <https://jasper.ai> .

jasper:FreeTrial a schema:Offer ;
    schema:name "Start Free Trial" ;
    schema:price "0" ;
    schema:priceCurrency "USD" ;
    schema:eligibleRegion "Worldwide" .

jasper:MarketingToolkit a schema:Product ;
    schema:name "AI Marketing Toolkit" ;
    schema:description "A comprehensive set of tools designed for marketers, including AI chat, document editing, and brand control." ;
    schema:brand jasper:Jasper .

jasper:CustomerStories a schema:WebPage ;
    schema:name "Customer Stories" ;
    schema:description "Real marketers, surreal results showcasing how businesses leverage Jasper to drive growth and success." ;
    schema:url "https://www.jasper.ai/customer-stories" .

jasper:TrustFoundation a schema:WebPage ;
    schema:name "Trust Foundation" ;
    schema:description "Enterprise-grade security and quality outputs ensuring data protection and privacy." ;
    schema:url "https://www.jasper.ai/trust" .

ex:GTM_AI_Platform a ex:Platform ;
    dct:title "GTM AI Platform" ;
    dct:description "A unified platform for go-to-market strategies that integrates AI functionalities to enhance productivity and efficiency." ;
    dct:subject ex:AI_Writing_Tools ;
    ex:hasFeature ex:Content_Creation, ex:Lead_Processing, ex:Account_Based_Marketing, ex:Translation_Localization, ex:Deal_Coaching_Forecasting ;
    ex:trustedBy ex:Siemens, ex:Lenovo, ex:Juniper_Networks, ex:Rubrick, ex:Gong, ex:TermoFisher_Scientific, ex:Urban_Outfitters ;
    prov:wasDerivedFrom <https://copy.ai> .

ex:Content_Creation a ex:Feature ;
    dct:description "Power your content engine with AI for SEO, thought leadership, and more." .

ex:Lead_Processing a ex:Feature ;
    dct:description "Automatically enrich, research, and engage marketing generated leads." .

ex:Account_Based_Marketing a ex:Feature ;
    dct:description "Get insights on accounts and create hyper-relevant assets at scale." .

ex:Translation_Localization a ex:Feature ;
    dct:description "Produce native speaker level translations in real-time." .

ex:Deal_Coaching_Forecasting a ex:Feature ;
    dct:description "Get insights from sales transcripts to score deals and suggest strategies." .

ex:Siemens a ex:Company ;
    dct:title "Siemens" .

ex:Lenovo a ex:Company ;
    dct:title "Lenovo" .

ex:Juniper_Networks a ex:Company ;
    dct:title "Juniper Networks" .

ex:Rubrick a ex:Company ;
    dct:title "Rubrick" .

ex:Gong a ex:Company ;
    dct:title "Gong" .

ex:TermoFisher_Scientific a ex:Company ;
    dct:title "Termo Fisher Scientific" .

ex:Urban_Outfitters a ex:Company ;
    dct:title "Urban Outfitters" .

ex:TextImprovement a ex:Feature ;
    rdfs:label "Text Improvement" ;
    rdfs:comment "Enhances writing quality and clarity." .

ex:PersonalizedSuggestions a ex:Feature ;
    rdfs:label "Personalized Suggestions" ;
    rdfs:comment "Provides tailored writing suggestions based on user input." .

ex:DataSecurity a ex:Feature ;
    rdfs:label "Data Security" ;
    rdfs:comment "Ensures user data privacy and does not sell data." .

ex:Organization1 a ex:Organization ;
    rdfs:label "Zoom" .

ex:Organization2 a ex:Organization ;
    rdfs:label "Databricks" .

ex:Organization3 a ex:Organization ;
    rdfs:label "Atlassian" .